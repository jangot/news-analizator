<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1><%= title %></h1>
<ul>
    <% data.forEach(function(it) { %>
        <li>
            Day :<%= it.day %>
            <ul>
                <% it.keywords_count.forEach(function(keyword) { %>
                    <li><%= keyword.keyword %> : <%= keyword.count %></li>
                <% }); %>
            </ul>
        </li>
    <% }); %>
</ul>
</body>
</html>
